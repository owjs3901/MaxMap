        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaxMap"
        mc:Ignorable="d"
        Title="MAX MAP" Height="350" Width="525">
    <StackPanel>
        <Button x:Name="GameSearchBtn" Click="GameSearch">게임찾기</Button>
        <Button x:Name="DrawGameBtn" Click="DrawGameScreen">화면표시</Button>
        <Image x:Name="ImgScreen" >
            <Image.Source>
                <BitmapImage DecodePixelWidth="200" UriSource="C:\Users\M\Desktop\title.png" />
            </Image.Source>
        </Image>
    </StackPanel>
</Window>